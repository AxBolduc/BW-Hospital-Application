<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.teamB.views.menus.PathfindingMenuController">
   <children>
      <JFXComboBox fx:id="startLocComboBox" promptText="Start Location" />
      <JFXComboBox fx:id="endLocComboBox" promptText="End Location" />
      <JFXButton fx:id="findPathBtn" text="Find Path" />
      <JFXButton fx:id="backBtn" cancelButton="true" onAction="#handleButtonAction" text="Back" />
   </children>
</VBox>
